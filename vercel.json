{
  "version": 2,
  "builds": [
    {
      "src": "api/greet.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/greet",
      "dest": "api/greet.py"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}